  "main": "index.js",
  "scripts": {
    "start": "dotenv -e .env node ./dist/server.js",
    "dev": "node --watch --experimental-strip-types --no-warnings --env-file .env ./src/server.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "node --experimental-strip-types --env-file .env ./src/database/seed.ts",
    "db:studio": "drizzle-kit studio",
    "pretest": "dotenv -e .env.test drizzle-kit migrate",
    "test": "dotenv -e .env.test vitest run",
    "lint": "biome check",
    "build": "tsc -p tsconfig.build.json"
