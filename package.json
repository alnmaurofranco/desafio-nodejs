    "@scalar/fastify-api-reference": "^1.34.2",
    "drizzle-orm": "^0.44.4",
    "fastify": "^5.5.0",
    "fastify-type-provider-zod": "^5.0.3",
    "pg": "^8.16.3",
    "pino-pretty": "^13.1.1",
    "supertest": "^7.1.4",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
